<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_newUserInfoFragment"
            app:destination="@id/newUserInfoFragment" />
        <action
            android:id="@+id/action_splashScreenFragment_to_pinLoginFragment"
            app:destination="@id/pinLoginFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginWithNumberFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.LoginWithNumberFragment"
        android:label="fragment_login_with_number"
        tools:layout="@layout/fragment_login_with_number" >
        <action
            android:id="@+id/action_loginWithNumberFragment_to_aboutLeoFragment"
            app:destination="@id/aboutLeoFragment" />
        <action
            android:id="@+id/action_loginWithNumberFragment_to_smsLoginFragment"
            app:destination="@id/smsLoginFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutLeoFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.AboutLeoFragment"
        android:label="fragment_about_leo"
        tools:layout="@layout/fragment_about_leo" />
    <fragment
        android:id="@+id/smsLoginFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.SmsLoginFragment"
        android:label="fragment_sms_login"
        tools:layout="@layout/fragment_sms_login" >
        <argument
            android:name="phoneNumber"
            app:argType="string" />
        <action
            android:id="@+id/action_smsLoginFragment_to_loginWithNumberFragment"
            app:destination="@id/loginWithNumberFragment" />
        <action
            android:id="@+id/action_smsLoginFragment_to_createPinFragment"
            app:destination="@id/pinCreatedFragment" />
    </fragment>
    <fragment
        android:id="@+id/pinLoginFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.PinLoginFragment"
        android:label="fragment_pin_login"
        tools:layout="@layout/fragment_pin_login" >
        <action
            android:id="@+id/action_pinLoginFragment_to_homeFragment"
            app:destination="@id/nav_home"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.leoapplication.presentation.ui.fragments.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_cardFragment"
            app:destination="@id/cardFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popEnterAnim="@android:anim/fade_in"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_nav_home_to_increaseBalanceFragment"
            app:destination="@id/increaseBalanceFragment" />
        <action
            android:id="@+id/action_nav_home_to_exportToFragment"
            app:destination="@id/exportToFragment" />
        <action
            android:id="@+id/action_nav_home_to_nav_collect"
            app:destination="@id/nav_collect"
            app:popUpTo="@id/nav_home" />
        <action
            android:id="@+id/action_nav_home_to_nav_investment"
            app:destination="@id/nav_investment" />
        <action
            android:id="@+id/action_nav_home_to_nav_edv"
            app:destination="@id/nav_edv" />
        <action
            android:id="@+id/action_nav_home_to_nav_menu"
            app:destination="@id/nav_menu" />
        <action
            android:id="@+id/action_nav_home_to_transactionDetailFragment"
            app:destination="@id/transactionDetailFragment" />
        <action
            android:id="@+id/action_nav_home_to_otherPaysFragment"
            app:destination="@id/otherPaysFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_collect"
        android:name="com.example.leoapplication.presentation.ui.fragments.collect.CollectFragment"
        android:label="fragment_collect"
        tools:layout="@layout/fragment_collect" />
    <fragment
        android:id="@+id/nav_investment"
        android:name="com.example.leoapplication.presentation.ui.fragments.investment.InvestmentFragment"
        android:label="fragment_investment"
        tools:layout="@layout/fragment_investment" />
    <fragment
        android:id="@+id/nav_edv"
        android:name="com.example.leoapplication.presentation.ui.fragments.edv.EdvFragment"
        android:label="fragment_edv"
        tools:layout="@layout/fragment_edv" />
    <fragment
        android:id="@+id/nav_menu"
        android:name="com.example.leoapplication.presentation.ui.fragments.profile.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/action_nav_menu_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_nav_menu_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_loginWithNumberFragment"
            app:destination="@id/loginWithNumberFragment" />
    </fragment>
    <fragment
        android:id="@+id/cardFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.home.CardFragment"
        android:label="fragment_card"
        tools:layout="@layout/fragment_card" >
        <argument
            android:name="phoneNumber"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/increaseBalanceFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.increaseBalance.IncreaseBalanceFragment"
        android:label="fragment_increase_balance"
        tools:layout="@layout/fragment_increase_balance" >
        <action
            android:id="@+id/action_increaseBalanceFragment_to_increaseWithGoogleFragment"
            app:destination="@id/increaseWithGoogleFragment" />
        <action
            android:id="@+id/action_increaseBalanceFragment_to_increaseOtherBankFragment"
            app:destination="@id/increaseOtherBankFragment" />
        <action
            android:id="@+id/action_increaseBalanceFragment_to_increaseOtherCardFragment"
            app:destination="@id/increaseOtherCardFragment" />
        <action
            android:id="@+id/action_increaseBalanceFragment_to_increaseWithMoneyFragment"
            app:destination="@id/increaseWithMoneyFragment" />
        <action
            android:id="@+id/action_increaseBalanceFragment_to_increaseWithSwiftFragment"
            app:destination="@id/increaseWithSwiftFragment" />
    </fragment>
    <fragment
        android:id="@+id/increaseWithGoogleFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.increaseBalance.IncreaseWithGoogleFragment"
        android:label="fragment_increase_with_google"
        tools:layout="@layout/fragment_increase_with_google" />
    <fragment
        android:id="@+id/increaseOtherBankFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.increaseBalance.IncreaseOtherBankFragment"
        android:label="fragment_increase_other_bank"
        tools:layout="@layout/fragment_increase_other_bank" />
    <fragment
        android:id="@+id/increaseOtherCardFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.increaseBalance.IncreaseOtherCardFragment"
        android:label="fragment_increase_other_card"
        tools:layout="@layout/fragment_increase_other_card" >
        <action
            android:id="@+id/action_increaseOtherCardFragment_to_increaseBalanceOtherCardsFragment"
            app:destination="@id/increaseBalanceOtherCardsFragment" />
    </fragment>
    <fragment
        android:id="@+id/increaseWithMoneyFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.IncreaseWithMoneyFragment"
        android:label="fragment_increase_with_money"
        tools:layout="@layout/fragment_increase_with_money" />
    <fragment
        android:id="@+id/increaseWithSwiftFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.increaseBalance.IncreaseWithSwiftFragment"
        android:label="fragment_increase_with_swift"
        tools:layout="@layout/fragment_increase_with_swift" />
    <fragment
        android:id="@+id/increaseBalanceOtherCardsFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.increaseBalance.IncreaseBalanceOtherCardsFragment"
        android:label="fragment_increase_balance_other_cards"
        tools:layout="@layout/fragment_increase_balance_other_cards" >
        <action
            android:id="@+id/action_increaseBalanceOtherCardsFragment_to_paymentSuccessFragment"
            app:destination="@id/paymentSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/exportToFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.transferToCard.ExportToFragment"
        android:label="fragment_export_to"
        tools:layout="@layout/fragment_export_to" >
        <action
            android:id="@+id/action_exportToFragment_to_exportToNewCardFragment"
            app:destination="@id/exportToNewCardFragment" />
    </fragment>
    <fragment
        android:id="@+id/pinCreatedFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.PinCreatedFragment"
        android:label="fragment_create_pin"
        tools:layout="@layout/fragment_pin_created" >
        <action
            android:id="@+id/action_pinCreatedFragment_to_pinLoginFragment"
            app:destination="@id/pinLoginFragment" />
    </fragment>
    <fragment
        android:id="@+id/newUserInfoFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.loginAuth.SignUpFragment"
        android:label="fragment_new_user_info"
        tools:layout="@layout/fragment_sign_up" >
        <argument
            android:name="phone"
            app:argType="string"/>
        <action
            android:id="@+id/action_newUserInfoFragment_to_loginWithNumberFragment"
            app:destination="@id/loginWithNumberFragment" />
    </fragment>
    <fragment
        android:id="@+id/transactionDetailFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.home.TransactionDetailFragment"
        android:label="fragment_transaction_detail"
        tools:layout="@layout/fragment_transaction_detail" />
    <fragment
        android:id="@+id/exportToNewCardFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.transferToCard.ExportToNewCardFragment"
        android:label="fragment_export_to_new_card"
        tools:layout="@layout/fragment_export_to_new_card" >
        <action
            android:id="@+id/action_exportToNewCardFragment_to_exportToMoneyFragment"
            app:destination="@id/exportToMoneyFragment" />
    </fragment>
    <fragment
        android:id="@+id/exportToMoneyFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.transferToCard.ExportToMoneyFragment"
        android:label="fragment_export_to_money"
        tools:layout="@layout/fragment_export_to_money" >
        <action
            android:id="@+id/action_exportToMoneyFragment_to_paymentSuccessFragment"
            app:destination="@id/paymentSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentSuccessFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.home.PaymentSuccessFragment"
        android:label="fragment_payment_success"
        tools:layout="@layout/fragment_payment_success" >
        <action
            android:id="@+id/action_paymentSuccessFragment_to_nav_home"
            app:destination="@id/nav_home" />
    </fragment>
    <fragment
        android:id="@+id/otherPaysFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.otherPays.OtherPaysFragment"
        android:label="fragment_other_pays"
        tools:layout="@layout/fragment_other_pays" >
        <action
            android:id="@+id/action_otherPaysFragment_to_exportToFragment"
            app:destination="@id/exportToFragment" />
        <action
            android:id="@+id/action_otherPaysFragment_to_mobileBalanceFragment"
            app:destination="@id/mobileBalanceFragment" />
        <action
            android:id="@+id/action_otherPaysFragment_to_wifiFragment"
            app:destination="@id/wifiFragment" />
    </fragment>
    <fragment
        android:id="@+id/mobileBalanceFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.otherPays.MobileBalanceFragment"
        android:label="fragment_mobile_balance"
        tools:layout="@layout/fragment_mobile_balance" >
        <action
            android:id="@+id/action_mobileBalanceFragment_to_exportToMobileFragment"
            app:destination="@id/exportToMobileFragment" />
        <action
            android:id="@+id/action_mobileBalanceFragment_to_monileNumberFragment"
            app:destination="@id/monileNumberFragment" />
    </fragment>
    <fragment
        android:id="@+id/exportToMobileFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.otherPays.ExportToMobileFragment"
        android:label="fragment_export_to_mobile"
        tools:layout="@layout/fragment_export_to_mobile" >
        <action
            android:id="@+id/action_exportToMobileFragment_to_paymentSuccessFragment"
            app:destination="@id/paymentSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/monileNumberFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.otherPays.MobileNumberFragment"
        android:label="fragment_monile_number"
        tools:layout="@layout/fragment_mobile_number" >
        <action
            android:id="@+id/action_monileNumberFragment_to_exportToMobileFragment"
            app:destination="@id/exportToMobileFragment" />
    </fragment>
    <fragment
        android:id="@+id/wifiFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.otherPays.WifiFragment"
        android:label="fragment_wifi"
        tools:layout="@layout/fragment_wifi" />
    <fragment
        android:id="@+id/abonentCodeFragment"
        android:name="com.example.leoapplication.presentation.ui.fragments.otherPays.AbonentCodeFragment"
        android:label="fragment_abonent_code"
        tools:layout="@layout/fragment_abonent_code" />

</navigation>